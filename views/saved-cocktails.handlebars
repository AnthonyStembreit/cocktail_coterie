{{> navbar}}

<!--find a way to make sure only that users saved cocktails are shown -->
{{#each saved_cocktail}}
    {{drinkName}}
    {{drinkImage}}
    <button id="read-more">read more</button>
    {{#unless read}}
      {{ingrediants read=true}}
      {{directions read=true}}
    {{/unless}}
    <button id="delete-cocktail">delete</button>
{{/each}}