<form>
    <div class="form-group">
        <label for="exampleInputPassword1">First Name</label>
        <input class="form-control" id="inputFirstName">
    </div>

    <div class="form-group">
        <label for="exampleInputPassword1">Last Name</label>
        <input class="form-control" id="inputLastName">
    </div>

    <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>

    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1">
    </div>

    <div class="form-group">
        <label for="exampleInputPassword1">Password Confirm</label>
        <input type="password" class="form-control" id="passwordConfirm">
    </div>

    <div class="form-group">
        <label for="exampleInputPassword1">Zoom Link</label>
        <input class="form-control" id="zoom">
        <small class="form-text text-muted">optional</small>
    </div>
    <label>Choose your preferred liqour</label>
    <small class="form-text text-muted">optional</small>
    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="rum">
        <label class="form-check-label" for="rum">
            Rum
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="tequila">
        <label class="form-check-label" for="tequila">
            Tequila
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="vodka">
        <label class="form-check-label" for="vodka">
           Vodka
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="whiskey">
        <label class="form-check-label" for="whiskey">
            Whiskey
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="burbon">
        <label class="form-check-label" for="burbon">
            Burbon
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="brandy">
        <label class="form-check-label" for="brandy">
           Brandy
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="gin">
        <label class="form-check-label" for="gin">
           Gin
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="scotch">
        <label class="form-check-label" for="scotch">
            Scotch
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="white">
        <label class="form-check-label" for="white">
           White-Wine
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="red">
        <label class="form-check-label" for="red">
            Red Wine
        </label>
    </div>

    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="import">
        <label class="form-check-label" for="import">
           Import Beer
        </label>
    </div>
    
    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="" id="domestic">
        <label class="form-check-label" for="domestic">
            Domestic Beer
        </label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    <p>or</p>
    <button><a href="/" data-toggle="modal" data-target="#myModal1">Login</a></button>
    <div class="modal fade" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        {{>login}}
    </div>
</form>